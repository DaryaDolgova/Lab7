#include <iostream>

using namespace std;

int main()
{
    setlocale(LC_CTYPE, "Russian");
int a,b,c;
cout<<"a= "; cin>>a;
cout<<"b= "; cin>>b;
cout<<"c= "; cin>>c;

if ((a+b>c)&&(a+c>b)&&(b+c>a)) {cout<<"Существует треугольник со сторонами a, b, c"<<endl;}
else {cout<<"Условие не выполняется"<<endl;}

return 0;
} 
